s://api.github.com\";\r\n\r\nconst GithubContext = React.createContext();\r\n\r\n// Provider, Consumer - GithubContext.Provider\r\n\r\nconst GithubProvider = ({ children }) => {\r\n  const [githubUser, setGithubUser] = useState(MockUser);\r\n  const [followers, setFollowers] = useState(MockFollower);\r\n  const [repos, setRepos] = useState(MockRepo);\r\n\r\n  // request loading\r\n  const [requests, setRequests] = useState(0);\r\n  const [loading, setLoading] = useState(false);\r\n\r\n  //   error\r\n  const [error, setError] = useState({ show: false, msg: \"\" });\r\n\r\n  const searchGithubUser = async (user) => {\r\n    // console.log(user);\r\n    toggleError();\r\n    setLoading(true);\r\n\r\n    const response = await axios(`${rootURL}/users/${user}`).catch((error) =>\r\n      console.log(error)\r\n    );\r\n    console.log(response);\r\n\r\n    if (response) {\r\n      setGithubUser(response.data);\r\n      const { login, followers_url } = response.data;\r\n\r\n      await Promise.allSettled([\r\n        axios(`${rootURL}/users/${login}/repos?per_page=100`),\r\n        axios(`${followers_url}?per_page=100`),\r\n      ])\r\n        .then((results) => {\r\n          const [repos, followers] = results;\r\n          const status = \"fulfilled\";\r\n          if (repos.status === status) {\r\n            setRepos(repos.value.data);\r\n          }\r\n          if (followers.status === status) {\r\n            setFollowers(followers.value.data);\r\n          }\r\n        })\r\n        .catch((err) => console.log(err));\r\n    } else {\r\n      toggleError(true, \"there is no user with that username\");\r\n    }\r\n  };\r\n\r\n  //check rate\r\n  const checkRequests = () => {\r\n    axios(`${rootURL}/rate_limit`)\r\n      .then(({ data }) => {\r\n        let {\r\n          rate: { remaining },\r\n        } = data;\r\n        setRequests(remaining);\r\n        if (remaining === 0) {\r\n          // throw an error & hide search button to tell requests are used up.\r\n          toggleError(true, \"sorry, you have exceeded your hourly rate limit!\");\r\n        }\r\n      })\r\n      .catch((error) => console.log(error));\r\n  };\r\n\r\n  function toggleError(show = false, msg = \"\") {\r\n    setError({ show, msg });\r\n  }\r\n\r\n  //errors\r\n  useEffect(checkRequests, []);\r\n\r\n  //   get intial user\r\n  useEffect(() => {\r\n    searchGithubUser(\"taurusilver7\");\r\n  }, []);\r\n\r\n  return (\r\n    <GithubContext.Provider\r\n      value={{\r\n        githubUser,\r\n        repos,\r\n        followers,\r\n        requests,\r\n        error,\r\n        searchGithubUser,\r\n        loading,\r\n      }}\r\n    >\r\n      {children}\r\n    </GithubContext.Provider>\r\n  );\r\n};\r\n\r\nexport const useGithubValue = () => useContext(GithubContext);\r\n\r\nexport { GithubProvider, GithubContext };\r\n","G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Info.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\context\\MockData\\MockUser.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\context\\MockData\\MockFollower.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\context\\MockData\\MockRepos.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Navbar.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\User.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Repos.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Search.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Charts\\index.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Charts\\Bar3D.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Charts\\ExampleChart.js",[],["112","113"],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Charts\\Doughnut2D.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Charts\\Column3D.js",[],"G:\\Javascript\\JS\\React_JS\\github-user\\src\\components\\Charts\\Pie3D.js",[],